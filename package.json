	"description": "@speackme/react is an easy-to-use components library for ReactJS & Tailwind CSS",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"start": "tailwindcss -o ./dist/tailwind.css --config ./tailwind.config.js --watch",
